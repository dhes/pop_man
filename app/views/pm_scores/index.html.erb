<h1>Listing pm_scores</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Measure</th>
      <th>Score</th>
      <th>Score date</th>
      <th>Numer</th>
      <th>Denom</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @pm_scores.each do |pm_score| %>
      <tr>
        <td><%= pm_score.measure %></td>
        <td><%= pm_score.score %></td>
        <td><%= pm_score.score_date.to_s[5..9]<<'-'<<pm_score.score_date.to_s[0..3] %></td>
        <td><%= pm_score.numer.to_i %></td>
        <td><%= pm_score.denom.to_i %></td>
        <td><%#= link_to 'Show', pm_score %></td>
        <td><%#= link_to 'Edit', edit_pm_score_path(pm_score) %></td>
        <td><%#= link_to 'Destroy', pm_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pm score', new_pm_score_path %>

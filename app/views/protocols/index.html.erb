<h1>Listing protocols</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Sex</th>
      <th>From age..</th>
      <th>To age..</th>
      <th>Problems</th>
      <th>Medications</th>
      <th>Tests</th>
      <th>Fact</th>
      <th>Numerator</th>
      <th>Denominator</th>
      <th>Score</th>
    </tr>
  </thead>

  <tbody>
    <% @protocols.each do |protocol| %>
      <tr>
        <td><%= protocol.name %></td>
        <td><%= protocol.sex %></td>
        <td><%= protocol.lowage.to_i/12 %></td>
        <td><%= protocol.highage.to_i/12 %></td>
        <td><%= protocol.problems %></td>
        <td><%= protocol.medications %></td>
        <td><%= protocol.tests %></td>
        <td><%= protocol.fact %></td>
        <td><%= protocol.numer %></td>
        <td><%= protocol.denom %></td>
        <td><%= protocol.score %></td>
        <td><%#= link_to 'Show', protocol %></td>
        <td><%#= link_to 'Edit', edit_protocol_path(protocol) %></td>
        <td><%#= link_to 'Destroy', protocol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Protocol', new_protocol_path %>
